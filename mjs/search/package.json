{
  "name": "@thinkeloquent/search",
  "version": "1.0.0",
  "description": "Enterprise-grade GitHub Search API client with scalability features",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./index.mjs",
  "bin": {
    "github-search": "./cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./index.mjs"
    },
    "./client": {
      "import": "./lib/SafeSearchClient.mjs"
    },
    "./endpoints": {
      "import": "./endpoints/index.mjs"
    },
    "./utils": {
      "import": "./utils/index.mjs"
    }
  },
  "scripts": {
    "start": "node cli.mjs",
    "test": "node --test --test-force-exit tests/index.test.mjs tests/unit/module-structure.test.mjs",
    "test:unit": "node --test tests/unit/module-structure.test.mjs",
    "test:integration": "node --test 'tests/integration/*.test.mjs'",
    "test:performance": "node --test 'tests/performance/*.test.mjs'",
    "dev": "node --watch cli.mjs",
    "lint": "eslint '**/*.mjs'",
    "format": "prettier --write '**/*.mjs'"
  },
  "keywords": [
    "github",
    "search",
    "api",
    "cli",
    "sdk",
    "enterprise",
    "scalable",
    "rate-limiting",
    "caching"
  ],
  "author": "GitHub Search API Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "lru-cache": "^10.1.0",
    "ms": "^2.1.3",
    "debug": "^4.3.4",
    "dotenv": "^16.4.1",
    "node-fetch": "^3.3.2",
    "inquirer": "^9.2.15",
    "cli-table3": "^0.6.3",
    "csv-writer": "^1.6.0",
    "jsonlines": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "@types/node": "^20.11.16"
  },
  "optionalDependencies": {
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "prom-client": "^15.1.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/sdk-node": "^0.46.0"
  }
}
