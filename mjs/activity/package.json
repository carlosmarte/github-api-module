{
  "name": "@thinkeloquent/github-sdk-activity",
  "version": "0.0.1",
  "description": "GitHub Activity API SDK and CLI - Complete implementation for events, notifications, stars, and watching",
  "type": "module",
  "main": "src/index.mjs",
  "bin": {
    "github-activity": "./bin/github-activity.mjs",
    "gha": "./bin/github-activity.mjs"
  },
  "exports": {
    ".": "./src/index.mjs",
    "./client": "./src/client/ActivityClient.mjs",
    "./models": "./src/models/types.mjs",
    "./api/events": "./src/api/events.mjs",
    "./api/notifications": "./src/api/notifications.mjs",
    "./api/feeds": "./src/api/feeds.mjs",
    "./api/stars": "./src/api/stars.mjs",
    "./api/watching": "./src/api/watching.mjs"
  },
  "scripts": {
    "start": "node ./bin/github-activity.mjs",
    "test": "node --test tests/**/*.test.mjs",
    "test:watch": "node --test --watch tests/**/*.test.mjs",
    "lint": "eslint src/**/*.mjs",
    "format": "prettier --write src/**/*.mjs"
  },
  "keywords": [
    "github",
    "api",
    "activity",
    "events",
    "notifications",
    "stars",
    "cli",
    "sdk",
    "rest"
  ],
  "author": "GitHub",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "cli-table3": "^0.6.3",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/github-api-module/monorepo.git",
    "directory": "mjs/activity"
  }
}
