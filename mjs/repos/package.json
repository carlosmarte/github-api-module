{
  "name": "@thinkeloquent/repos",
  "version": "1.0.0",
  "description": "GitHub Repository API client and CLI - Full-featured repository management",
  "type": "module",
  "main": "index.mjs",
  "bin": {
    "gh-repo": "./cli.mjs"
  },
  "exports": {
    ".": "./index.mjs",
    "./client": "./src/client/RepoClient.mjs",
    "./types": "./types/index.d.ts"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "echo 'Linting not configured yet'",
    "build": "echo 'Build process not needed for ES modules'",
    "start": "node cli.mjs",
    "dev": "node --watch cli.mjs"
  },
  "keywords": [
    "github",
    "api",
    "repository",
    "repo",
    "cli",
    "sdk",
    "rest",
    "git",
    "collaboration",
    "webhooks",
    "security",
    "management"
  ],
  "author": "GitHub API Module",
  "license": "MIT",
  "dependencies": {
    "@thinkeloquent/npm-api-rate-limiter": "^1.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.10",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.0",
    "@types/node": "^20.5.0",
    "jest": "^29.6.0",
    "nock": "^13.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/github-api-module/repos.git"
  },
  "homepage": "https://github.com/github-api-module/repos#readme",
  "bugs": {
    "url": "https://github.com/github-api-module/repos/issues"
  },
  "files": [
    "src/",
    "types/",
    "examples/",
    "index.mjs",
    "cli.mjs",
    "README.md"
  ]
}
